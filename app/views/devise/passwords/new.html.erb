<div class="page-header">
  <h1 style="text-align: center">Maestro Enrique Buenaventura</h1>
</div>
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 style="text-align: center">Recordar contraseña</h3>
  </div>
  <div class="panel-body">

<%= form_for(resource, as: resource_name, url: password_path(resource_name), html: { method: :post }) do |f| %>
  <%= devise_error_messages! %>


   



  <div class="form-group col-xs-9">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, placeholder: 'Digita tu correo electronico',:class=>"form-control" %>
  </div>

  <div class="form-group col-xs-9">
    <%= f.submit "Recuperar contraseña", :class=>"btn btn-primary" %>
  </div>
<% end %>
<div class="form-group col-xs-9">
<%= render "devise/shared/links" %>
</div>
  </div>
</div>
