
<div class="page-header">
  <h1 style="text-align: center">Maestro Enrique Buenaventura</h1>
</div>
<hr>
<div class="panel panel-default">
  <div class="panel-heading">
    <h2 style="text-align: center">Iniciar sesión</h2>
  </div>
  <div class="panel-body">
  <div class="container">

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="form-group col-xs-9">
    <label for="exampleInputEmail1">Usuario</label>
    <%= f.email_field :email, autofocus: true, placeholder: 'Digita tu email',:class=>"form-control"%>
  </div>
  <div class="form-group col-xs-9">
    <label for="exampleInputPassword1">Contraseña</label>
    <%= f.password_field :password, autocomplete: "off", placeholder: 'Digita tu Contraseña' ,:class=>"form-control" %>
  </div>

<div class="form-group col-xs-9">
  <% if devise_mapping.rememberable? -%>
      <%= f.check_box :remember_me %>
      <%= f.label :recordarme %>
  <% end -%>
</div>

<div class="form-group col-xs-9">
    <%= f.submit "Iniciar sesion", :class=>"btn btn-primary" %>
</div>
<% end %>
<div class="form-group col-xs-9">
<%= render "devise/shared/links" %>
</div>
</div>

  </div>
</div>
