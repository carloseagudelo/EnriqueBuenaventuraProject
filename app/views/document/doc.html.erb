<iframe id="iframe" src="/pdfjs/web/viewer.html?file=<%= @document.attachment_url %>" style="position:absolute;top:0;left:0;width:100%;height:100%;">
</iframe>

<div class="row" style="position:fixed; bottom:0px; left:0px;">
	<div class="col-xs-12 col-md-12" >
	    <%= link_to "Seleccionar otro Documento", document_list_path, :class=>"btn btn-primary " %>
	</div> 
</div>

<p hidden id="nameDoc"><%= @document.name %></p>
<p hidden id="state"><%= @document.status %></p>
<p hidden id="year"><% t = @document.date %><%= t.strftime("%Y") %></p>
